<p id="notice"><%= notice %></p>

<h1>Tipo Campos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Tipo dato</th>
      <th>Descripcion</th>
      <th>Tiene respuesta</th>
      <th>Tipo seleccion</th>
      <th>Tipo contenido</th>
      <th>User created</th>
      <th>User updated</th>
      <th>Usr grab</th>
      <th>Usr modi</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_campos.each do |tipo_campo| %>
      <tr>
        <td><%= tipo_campo.nombre %></td>
        <td><%= tipo_campo.tipo_dato %></td>
        <td><%= tipo_campo.descripcion %></td>
        <td><%= tipo_campo.tiene_respuesta %></td>
        <td><%= tipo_campo.tipo_seleccion_id %></td>
        <td><%= tipo_campo.tipo_contenido_id %></td>
        <td><%= tipo_campo.user_created_id %></td>
        <td><%= tipo_campo.user_updated_id %></td>
        <td><%= tipo_campo.usr_grab %></td>
        <td><%= tipo_campo.usr_modi %></td>
        <td><%= tipo_campo.estado %></td>
        <td><%= link_to 'Show', tipo_campo %></td>
        <td><%= link_to 'Edit', edit_tipo_campo_path(tipo_campo) %></td>
        <td><%= link_to 'Destroy', tipo_campo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Campo', new_tipo_campo_path %>
